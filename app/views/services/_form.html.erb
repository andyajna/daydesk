<%= simple_form_for([@supplier, @service]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :desktype %>
    <%= f.input :deskrate %>
    <%= f.input :wifi %>
    <%= f.input :wifilimit %>
    <%= f.input :printing %>
    <%= f.input :printingdesc %>
    <%= f.input :sendfax %>
    <%= f.input :receivefax %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
